<?xml version="1.0" encoding="utf-8" ?>
<views xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="https://github.com/HuangZhiBin/Petral-UI/raw/2.2.0/Example/Petral/petral-ui.xsd">
    <view left="0" right="0" top="0" bottom="0" backgroundColor="white">
        <tableview id="tableview1" left="0" top="0" width="100%" height="100%" backgroundColor="#d6d6d6" p-rows="20">
            <cell height="80">
                <imageview id="cellImage" image="setting" width="50" height="50" left="15" yCenter="0" cornerRadius="25" masksToBounds="true" />
                <label id="cellLabel" left="cellImage;15" right="15" height="20" top="cellImage" text="this is cell text" />
                <label id="cellSubLabel" left="cellImage;15" right="15" height="20" top="cellLabel;10" text="this is cell sub text" />
            </cell>
        </tableview>
    </view>
</views>
