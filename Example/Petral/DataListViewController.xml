<?xml version="1.0" encoding="utf-8" ?>
<views xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://www.koudaikr.cn/petral.xsd">
    <view left="0" right="0" top="0" bottom="0" backgroundColor="white">
        <tableview id="tableview1" left="0" top="0" width="100%" bottom="70" backgroundColor="#f9f9f9" p-sections="2" p-rows="3">
            <section-header height="40" backgroundColor="#d9d9d9">
                <label id="sectionHeaderLabel" width="100%" height="20" center="0" text="this is section header" align="center" textColor="black" />
            </section-header>
            <cell height="80">
                <imageview id="cellImage" image="setting" width="50" height="50" left="15" yCenter="0" />
                <label id="cellLabel" left="cellImage;15" right="15" height="20" yCenter="0" text="this is cell text" />
            </cell>
        </tableview>
        <button id="changeBtn" title="切换数据" backgroundColor="#16a051" backgroundColorHighlighted="#83b001" bottom="10" masksToBounds="true" borderRadius="6" height="44" xCenter="0" width="90%" />
    </view>
</views>
